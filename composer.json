{
	"name": "atomicplan/plate-ttc-additions",
	"type": "typo3-cms-extension",
	"description": "Plate tt_content additions - provides additional generic fields, which can be used in extensions or a theme",
	"license": "GPL-2.0-or-later",
	"keywords": ["TYPO3 CMS", "Integrators", "Site Extension", "Site Package", "TypoScript"],
	"require": {
		"php": "^7.4 || ^8.0",
		"typo3/cms-core": "^11.0 || ^12.0",
		"blueways/bw-icons": "^3.1"
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "plate_ttc_additions"
		}
	},
	"autoload": {
		"psr-4": {
			"Atomicplan\\PlateTtcAdditions\\": "Classes/"
		}
	},
	"config": {
		"sort-packages": true
	}
}
